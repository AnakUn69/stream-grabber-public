{
    "manifest_version": 3,
    "name": "Stream Grabber",
    "version": "0.1.0",
    "description": "Grab stream links from hellspy.to video tags",
    "author": "Vagis",
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "clipboardWrite"
    ],
    "host_permissions": [
        "https://api.hellspy.to/*"
    ],
    "icons": {
        "16": "icons/icon.png",
        "48": "icons/icon.png",
        "128": "icons/icon.png"
    }
}